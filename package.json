{
  "name": "chrome-extension-custom-new-tab",
  "version": "0.0.1",
  "description": "Customized new tab",
  "main": "src/index.js",
  "private": true,
  "browserslist": [
    "chrome >= 87"
  ],
  "engines": {
    "node": ">=14.15"
  },
  "scripts": {
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "clean": "run-p clean:*",
    "lint:styles": "stylelint '**/*.less'",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:lockfile": "lockfile-lint --type npm --path package-lock.json --allowed-hosts npm --validate-https",
    "lint": "run-p lint:*",
    "prebuild": "run-s clean",
    "build": "webpack",
    "start": "webpack-dev-server --inline --open",
    "test:js": "jest --coverage",
    "test": "run-p test:*"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rognan/chrome-extension-custom-new-tab.git"
  },
  "author": "Thor Andreas Rognan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rognan/chrome-extension-custom-new-tab/issues"
  },
  "homepage": "https://github.com/rognan/chrome-extension-custom-new-tab#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@types/react": "^17.0.6",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.27.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "less": "^4.1.1",
    "less-loader": "^7.2.1",
    "lockfile-lint": "^4.6.2",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-loader": "^4.1.0",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@sentry/browser": "^5.27.4",
    "core-js": "^3.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-live-clock": "^4.0.5"
  }
}
