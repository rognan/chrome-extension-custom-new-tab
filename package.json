{
  "name": "chrome-extension-custom-new-tab",
  "version": "0.0.1",
  "description": "Customized new tab",
  "main": "src/index.js",
  "private": true,
  "sideEffects": [
    "*.css"
  ],
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "engines": {
    "node": ">=16.4"
  },
  "scripts": {
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "clean": "run-p clean:*",
    "lint:styles": "stylelint '**/*.css'",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:lockfile": "lockfile-lint --type npm --path package-lock.json --allowed-hosts npm --validate-https",
    "lint": "run-p lint:*",
    "prebuild": "run-s clean",
    "build": "webpack",
    "pretest": "run-s build",
    "test:e2e": "playwright test -c ./scripts/playwright.config.ts",
    "test:js": "jest -c ./scripts/jest.config.js",
    "test": "run-p test:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rognan/chrome-extension-custom-new-tab.git"
  },
  "author": "Thor Andreas Rognan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rognan/chrome-extension-custom-new-tab/issues"
  },
  "homepage": "https://github.com/rognan/chrome-extension-custom-new-tab#readme",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@playwright/test": "^1.15.2",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.27",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^25.0.5",
    "eslint-plugin-react": "^7.26.1",
    "eslint-webpack-plugin": "^3.0.1",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.2.5",
    "lighthouse": "^8.5.1",
    "lockfile-lint": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "playwright": "^1.15.2",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "typescript": "^4.4.3",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0"
  },
  "dependencies": {
    "core-js": "^3.18.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-live-clock": "^5.2.0"
  }
}
