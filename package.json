{
  "name": "chrome-extension-custom-new-tab",
  "version": "0.0.1",
  "description": "Customized new tab",
  "main": "src/index.js",
  "private": true,
  "sideEffects": [
    "*.css"
  ],
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "engines": {
    "node": ">=14.15"
  },
  "scripts": {
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "clean": "run-p clean:*",
    "lint:styles": "stylelint '**/*.css'",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:lockfile": "lockfile-lint --type npm --path package-lock.json --allowed-hosts npm --validate-https",
    "lint": "run-p lint:*",
    "prebuild": "run-s clean",
    "build": "webpack",
    "start": "webpack-dev-server --inline --open",
    "pretest": "run-s build",
    "test:e2e": "playwright test -c ./scripts/playwright.config.ts",
    "test:js": "jest -c ./scripts/jest.config.js",
    "test": "run-p test:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rognan/chrome-extension-custom-new-tab.git"
  },
  "author": "Thor Andreas Rognan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rognan/chrome-extension-custom-new-tab/issues"
  },
  "homepage": "https://github.com/rognan/chrome-extension-custom-new-tab#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@playwright/test": "^1.12.3",
    "@types/react": "^17.0.11",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.29.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.24.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.5",
    "lighthouse": "^8.0.0",
    "lockfile-lint": "^4.6.2",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "playwright": "^1.12.3",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-webpack-plugin": "^2.2.2",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "core-js": "^3.15.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-live-clock": "^4.0.5"
  }
}
