{
  "name": "chrome-extension-custom-new-tab",
  "version": "0.0.1",
  "description": "Customized new tab",
  "main": "src/index.js",
  "browserslist": [
    "chrome >= 78"
  ],
  "engines": {
    "node": ">=10.13"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint:styles": "stylelint '**/*.less'",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:lockfile": "lockfile-lint --type npm --path package-lock.json --allowed-hosts npm --validate-https",
    "lint": "run-p lint:*",
    "prebuild": "run-s clean",
    "build": "cross-env NODE_ENV=production webpack -p --optimize-minimize",
    "start": "webpack-dev-server --inline --open",
    "test:js": "jest",
    "test": "run-p test:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rognan/chrome-extension-custom-new-tab.git"
  },
  "author": "Thor Andreas Rognan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rognan/chrome-extension-custom-new-tab/issues"
  },
  "homepage": "https://github.com/rognan/chrome-extension-custom-new-tab#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@types/react": "^16.9.53",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-react": "^7.21.4",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.5.3",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "lockfile-lint": "^4.3.7",
    "lodash": "^4.17.20",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@sentry/browser": "^5.27.1",
    "core-js": "^3.6.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-live-clock": "^4.0.5"
  }
}
